[include]
    path = ~/.secret_git_config
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
[alias]
    ocmmit = commit
    cm = commit
	puhs = push
	puh = push
    ps = push
    pul = pull
    pl = pull
	stauts = status
	stats = status
    sts = status
    st = status
    br = branch
	brancg = branch
	dif = diff
	cehckout = checkout
	co = checkout
	cecout = checkout
	checout = checkout
    chekcout = checkout
	squash = "!f(){ git reset --soft HEAD~${1} && git commit --edit -m\"$(git log --format=%B --reverse HEAD..HEAD@{1})\"; };f"
	fixup = "!f(){ git commit -a --allow-empty-message -m \"\" && git squash 2; };f"
[pull]
	rebase = true
[init]
	defaultBranch = main
